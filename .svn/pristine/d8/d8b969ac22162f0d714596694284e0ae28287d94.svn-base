	<select id="integrityVerificationForUserBandwidth" parameterType="long" resultType="int">
		select count(1) from idc_isms_base_user_hh t where t.userid = #{userId} and t.houseid = #{houseId}
	</select>
	<select id="integrityVerificationForUserIp" parameterType="com.aotain.cu.serviceapi.model.UserInformation" resultType="int">
          select count(1) from idc_isms_base_house_ipseg t
		where t.houseid = to_number(#{houseIDs}) and t.username = #{unitName}
		<if test="idType != null and idNumber != null">
			AND (t.idtype = #{idType} and t.idnumber = #{idNumber})
		</if>
	</select>
	<select id="integrityVerificationForUserFrames" parameterType="com.aotain.cu.serviceapi.model.UserInformation" resultType="int">
		select count(1) from idc_isms_base_user_frame t 
		where t.houseid = to_number(#{houseIDs}) and t.unitname = #{unitName}
		<if test="idType != null and idNumber != null">
          AND (t.idtype = #{idType} and t.idnumber = #{idNumber})
		</if>
	</select>
