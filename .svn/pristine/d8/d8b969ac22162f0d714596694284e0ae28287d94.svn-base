  </select>
  
  <update id="submitUser" parameterType="com.aotain.cu.serviceapi.model.UserInformation">
    UPDATE IDC_ISMS_BASE_USER
    <set>
      <if test="dealFlag!=null">
        DEAL_FLAG=#{dealFlag},
      </if>
      <if test="verificationResult!=null">
        VERIFICATION_RESULT=#{verificationResult},
      </if>
      UPDATE_TIME=sysdate,
    </set>
    WHERE USERID=#{userId}
    <if test="isRevoke!=0 ">
      and DEAL_FLAG!=0
    </if>
  </update>
