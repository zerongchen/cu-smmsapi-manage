  <select id="getRptHouse" parameterType="com.aotain.cu.serviceapi.model.HouseInformation" resultMap="RptHouseBaseResultMap">
    SELECT
    HOUSEID,JYZID,HOUSEIDSTR,HOUSENAME,HOUSETYPE,HOUSEPROVINCE,HOUSECITY,HOUSECOUNTY
